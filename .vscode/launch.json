	"version": "0.2.0",
	"configurations": [
		{
			"request": "launch",
			"name": "ask cloudflare AI",
			"type": "node",
			"program": "${workspaceFolder}/ask_cf_ai.ts",
			"cwd": "${workspaceFolder}",
			"env": {},
			"runtimeExecutable": "deno.EXE",
			"runtimeArgs": [
				"run",
				"--allow-all"
			],
			"attachSimplePort": 9229
		},
		{
			"request": "launch",
			"name": "Start SSE Test Server",
			"type": "node",
			"program": "${workspaceFolder}/sse_test.ts",
			"cwd": "${workspaceFolder}",
			"env": {},
			"runtimeExecutable": "deno.EXE",
			"runtimeArgs": [
				"run",
				"--watch",
				"--unstable",
				"--inspect-wait",
				"--inspect-brk",
				"--allow-all"
			],
			"attachSimplePort": 9229
		},
		{
			"request": "launch",
			"name": "vless",
			"type": "node",
			"program": "${workspaceFolder}/vless.ts",
			"cwd": "${workspaceFolder}",
			"env": {
				"USER_UUID": "bcaa12d6-1f1f-4f32-84b6-b701de2e1eec",
			},
			"runtimeExecutable": "deno.EXE",
			"runtimeArgs": [
				"run",
				// "--watch",
				"--unstable",
				"--inspect-wait",
				"--inspect-brk",
				"--allow-all"
			],
			"attachSimplePort": 9229
		}
	]
}